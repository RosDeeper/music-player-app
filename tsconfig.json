{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "target": "ES6",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "jsx": "react-native",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        ".src/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d-ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
